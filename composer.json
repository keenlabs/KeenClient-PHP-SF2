{
    "name":         "keen-io/keen-io-bundle",
    "type":         "symfony-bundle",
    "description":  "Symfony Bundle for Keen IO",
    "keywords":     ["keen", "keen.io", "events", "analytics", "symfony", "bundle" ],
    "homepage":     "https://github.com/keenlabs/KeenClient-PHP-Sf2",
    "license":      "MIT",
    "authors":      [
        {
            "name":     "Keith Kirk",
            "email":    "keith@kmfk.io",
            "role":     "developer",
            "homepage": "http://kmfk.io"
        }
    ],
    "support":     {
        "email": "keith@kmfk.io"
    },
    "require":     {
        "php": ">=7.2",
        "keen-io/keen-io": "~2.5",
        "symfony/config": "^3.4.26 || ^4.3",
        "symfony/dependency-injection": "^3.4.26 || ^4.3",
        "symfony/http-kernel": "^3.4.26 || ^4.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.4",
        "symfony/yaml": "^3.4.26 || ^4.3",
        "phpunit/phpunit": "^6"
    },
    "autoload": {
        "psr-4": {
            "KeenIO\\Bundle\\KeenIOBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KeenIO\\Bundle\\KeenIOBundle\\Tests\\": "tests/"
        }
    }
}
